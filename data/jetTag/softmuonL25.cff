# b HLT modules for Level 2.5 
module  hltBSoftmuonL25filter = hltJetTag from "HLTrigger/btau/data/jetTag/hltJetTag.cfi"
replace hltBSoftmuonL25filter.JetTag = hltBSoftmuonL25BJetTags
replace hltBSoftmuonL25filter.MinTag = 0.5  # 1 if a lepton is found in the jet, 0 otherwise

# Event reconstruction needed for trigger.
sequence hltBSoftmuonL25reco = { 
    hltBSoftmuonL25jetselection & 
    l2muonrecoNocand, hltBSoftmuonL25TagInfos, hltBSoftmuonL25BJetTags
}
