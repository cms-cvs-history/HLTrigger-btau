# b HLT modules for Level 2.5 
module  hltL25bmu = hltJetTag from "HLTrigger/btau/data/jetTag/hltJetTag.cfi"
replace hltL25bmu.JetTag = softMuJetTagsL25
replace hltL25bmu.MinTag = 0.5  # 1 if a lepton is found in the jet, 0 otherwise

# Event reconstruction needed for trigger.
sequence preL25bRecoMu = { 
    softmuonJetSel &
    l2muonrecoNocand,
    associatorL25Mu, 
    softMuJetTagsL25 
}
