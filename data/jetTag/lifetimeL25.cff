# b HLT modules for Level 2.5 
module  hltBLifetimeL25filter = hltJetTag from "HLTrigger/btau/data/jetTag/hltJetTag.cfi"
replace hltBLifetimeL25filter.JetTag = hltBLifetimeL25BJetTags
replace hltBLifetimeL25filter.MinTag = 3.5

# Reconstruction sequences needed before trigger.
sequence hltBLifetimeL25reco = { 
    (hltBLifetimeL25jetselection & hltBLifetimeL25tracking),
    hltBLifetimeL25Associator, 
    hltBLifetimeL25TagInfos,
    hltBLifetimeL25BJetTags
}
