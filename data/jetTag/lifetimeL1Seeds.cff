# L1 triggers to be used for b lifetime tag HLT.

# Choose the L1 trigger bits to use
module  bhltL1filter = hltLevel1GTSeed from "HLTrigger/HLTfilters/data/hltLevel1GTSeed.cfi"
replace bhltL1filter.L1SeedsLogicalExpression = "L1_SingleJet150 OR L1_DoubleJet100 OR L1_TripleJet50 OR L1_QuadJet40 OR L1_HTT300"

sequence L1forBlifeHLT = { bhltL1filter }
